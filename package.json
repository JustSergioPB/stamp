{
  "name": "stamp",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "dotenv -- turbo dev",
    "lint": "turbo lint",
    "test": "turbo test",
    "e2e:headless": "turbo e2e:headless",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "prettier": "3.2.5",
    "ts-node": "10.9.2",
    "turbo": "1.13.3"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.5.1",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "dotenv-cli": "7.4.2"
  }
}
